@model VictuZ_Lars.Models.Activity

@{
    ViewData["Title"] = "Registered Users";
}

<h1>Registered Users for @Model.Name</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model.RegisteredUsers)
        {
            <tr>
                <td>@user.FirstName @user.LastName</td>
                <td>@user.Email</td>
            </tr>
        }
    </tbody>
</table>
